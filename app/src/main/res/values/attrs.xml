<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DeviceItemView">
        <attr name="device_name" format="string" />
        <attr name="device_icon" format="reference" />
        <attr name="item_type">
            <enum name="helment" value="1" />
            <enum name="rocker" value="2" />
            <enum name="cadence" value="3" />
            <enum name="heart" value="4" />
            <enum name="wheel" value="5" />
        </attr>
    </declare-styleable>
    <declare-styleable name="WebViewWithProgress">
        <attr name="progressStyle" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="circle" value="1" />
        </attr>
        <attr name="barHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ShSwitchView">
        <attr name="tintColor" format="reference|color" />
        <attr name="outerStrokeWidth" format="reference|dimension" />
        <attr name="shadowSpace" format="reference|dimension" />
    </declare-styleable>
    <declare-styleable name="loopview">
        <attr name="minWidth" format="dimension" />
        <attr name="midTextSize" format="dimension" />
        <attr name="position" format="integer" />
        <attr name="offset" format="integer" />
    </declare-styleable>
    <declare-styleable name="TopBar">
        <attr name="top_bar_left_icon" format="reference" />
        <attr name="top_bar_left_text" format="string" />
        <attr name="top_bar_left_text_color" format="color" />
        <attr name="top_bar_left_is_show" format="boolean" />
        <attr name="top_bar_left_text_size" format="dimension" />
        <attr name="top_bar_left_select_text_color" format="color" />
        <attr name="top_bar_left_second_icon" format="reference" />
        <attr name="top_bar_left_second_icon_is_show" format="boolean" />
        <attr name="top_bar_title_text" format="string" />
        <attr name="top_bar_title_text_color" format="color" />
        <attr name="top_bar_title_is_show" format="boolean" />
        <attr name="top_bar_title_text_size" format="dimension" />
        <attr name="top_bar_right_icon" format="reference" />
        <attr name="top_bar_right_text" format="string" />
        <attr name="top_bar_right_text_color" format="color" />
        <attr name="top_bar_right_is_show" format="boolean" />
        <attr name="top_bar_right_text_size" format="dimension" />
        <attr name="top_bar_right_select_text_color" format="color" />
        <attr name="top_bar_right_second_icon" format="reference" />
        <attr name="top_bar_right_second_icon_is_show" format="boolean" />
    </declare-styleable>

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color" />
        <attr name="is_draw_border" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AutofitTextView">

        <!-- Minimum size of the text. -->
        <attr name="minTextSize" format="dimension" />
        <!--
        Amount of precision used to calculate the correct text size to fit within its
        bounds. Lower precision is more precise and takes more time.

        -->
        <attr name="precision" format="float" />
        <!-- Defines whether to automatically resize text to fit to the view's bounds. -->
        <attr name="sizeToFit" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DraggableFlagView">
        <attr name="flagViewColor" format="color" />
    </declare-styleable>
    <declare-styleable name="UnitSwitchView">
        <attr name="left_tv" format="string" />
        <attr name="right_tv" format="string" />
    </declare-styleable>

    <declare-styleable name="ContactSideBar">
        <attr name="character_text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="UIProgressBar">
        <attr name="finished_drawable" format="reference" />
        <attr name="unfinished_drawable" format="reference" />
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="progress_max" format="float" />
        <attr name="current_progress" format="float" />
        <attr name="bottom_text" format="string" />
        <attr name="top_text" format="string" />
    </declare-styleable>

    <!--骑行界面-->
    <attr name="TextColor" format="reference" />
    <attr name="BackgroundColor" format="reference" />
    <attr name="LineColor" format="reference" />
    <attr name="ActionbarColor" format="reference" />
    <attr name="TextTableColor" format="reference" />
    <attr name="TextProgressColor" format="reference" />
    <attr name="TextCadHeartColor" format="reference" />
    <attr name="VoiceIcon" format="reference" />
    <attr name="MusicIcon" format="reference" />
    <attr name="RiddingLeftIcon" format="reference" />
    <attr name="ProgressUIBackground" format="reference" />
    <attr name="MenuLeftIcon" format="reference" />
    <attr name="CadIcon" format="reference" />
    <attr name="HeartIcon" format="reference" />
    <attr name="CameraIcon" format="reference" />

    <!--云信uikit配置-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ImageViewEx">
        <attr name="exiv_default_image_res" format="reference" />
    </declare-styleable>
    <!--云信uikit配置-->
    <!-- 下拉刷新 -->
    <declare-styleable name="pullrefresh">
        <attr name="actionViewId" format="reference" />
        <attr name="toolViewId" format="reference" />
    </declare-styleable>
    <!--锁屏界面仪表盘-->
    <declare-styleable name="UIProgressBar2">
        <attr name="text_size2" format="dimension" />
        <attr name="text_color2" format="color" />
        <attr name="progress_max2" format="float" />
        <attr name="current_progress2" format="float" />
        <attr name="bottom_text2" format="string" />
        <attr name="top_text2" format="string" />
    </declare-styleable>

</resources>
